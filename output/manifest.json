{
  "manifest_version": 2,
  "name": "StandUpOrYoullTurnIntoABlobFish",
  "short_name": "StandUpOrYoullTurnIntoABlobFish",
  "version": "2.3.0",
  "description": "Fight the Blob, Stand for Your Health!",
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "permissions": ["storage", "alarms", "notifications"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "options/index.html",
    "default_title": "__MSG_extensionName__",
    "browser_style": true,
    "default_icon": {
      "32": "icons/32.png",
      "48": "icons/48.png",
      "96": "icons/96.png",
      "128": "icons/128.png"
    }
  }
}
